{
  "name": "@stepkit/local",
  "version": "0.0.0",
  "type": "module",
  "main": "./main.cjs",
  "module": "./main.js",
  "types": "./main.d.ts",
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "exports": {
    ".": {
      "types": {
        "import": "./main.d.ts",
        "require": "./main.d.cts"
      },
      "import": "./main.js",
      "require": "./main.cjs"
    }
  },
  "scripts": {
    "build": "tsdown --config tsdown.config.ts",
    "dpdm": "dpdm --exit-code circular:1 src/main.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@stepkit/core": "workspace:*",
    "@stepkit/sdk-tools": "workspace:*"
  },
  "devDependencies": {
    "dpdm": "^3.14.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.5",
    "zod": "^4.1.12"
  }
}
